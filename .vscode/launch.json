    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch Device Server",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/deviceserver/DeviceServer",
            "args": ["-d", "-c", "${workspaceFolder}/resources/config"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch ServerLibraryTestCrypto",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/test/serverlib/ServerLibraryTestCrypto",
